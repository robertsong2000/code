#
# Processor
#
CONFIG_EM_ARCH=mips
CONFIG_EM_MIPS=y
CONFIG_EM_CPU=emma3t

#
# Toolchain for Userland testsuite
#
EM_USR_CROSS=mips-sony-linux-dev-

#
# Console
#
CONFIG_EM_CONSOLE_IRQ=56

#
# Feature
#
CONFIG_EM_CALLSTACK="kstack,ustack"
CONFIG_EM_DISASSEMBLE=y
CONFIG_EM_DISASSEMBLE_GNU=y
CONFIG_EM_DEMANGLE=y
CONFIG_EM_DEMANGLE_GNU=y
CONFIG_EM_LICENSE="Dual BSD/GPL"
CONFIG_EM_LOGFILENAME="/root/exception.txt"
